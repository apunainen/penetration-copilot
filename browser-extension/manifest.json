{
    "description": "Dumps current page to localhost",
    "manifest_version": 3,
    "name": "LocalDump",
    "version": "1.0",
    "icons": {
        "48": "icons/icon-48.png"
    },
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "action": {
        "default_icon": "icons/icon-32.png"
    },
    "permissions": [
        "tabs",
        "webRequest"
    ],
    "host_permissions": [
        "<all_urls>"
    ]
}