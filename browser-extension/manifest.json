{
    "description": "Dumps current page to localhost",
    "manifest_version": 2,
    "name": "LocalDump",
    "version": "1.0",

    "homepage_url": "https://localhost",

    "icons": {
        "48": "icons/icon-48.png"
    },
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "browser_action": {
        "default_icon": "icons/icon-48.png",
        "default_title": "Penetrate"
    },
    "permissions": [
        "tabs",
        "webRequest",
        "<all_urls>"
    ]
}